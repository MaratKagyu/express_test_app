{
  "name": "express_test_app",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migration/${npm_config_name} -d ./src/config/data-source.ts",
    "migration:migrate": "npx typeorm-ts-node-commonjs migration:run -d ./src/config/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/config/data-source.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaratKagyu/express_test_app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaratKagyu/express_test_app/issues"
  },
  "homepage": "https://github.com/MaratKagyu/express_test_app#readme",
  "description": "",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
